definition
	ADMIN_ENTRY[at0000] matches {	-- Inpatient administration
		other_participations cardinality matches {0..*; unordered} matches {
			PARTICIPATION occurrences matches {0..*} matches {
				function matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0150, 	-- Attending doctor
							at0151, 	-- Referring doctor
							at0152, 	-- Consulting doctor
							at0153, 	-- Admitting doctor
							at0154]	-- Discharge doctor
						}
					}
				}
			}
		}
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {2..*; unordered} matches {
					ELEMENT[at0131] occurrences matches {0..1} matches {	-- Visit number
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0143] occurrences matches {0..1} matches {	-- Movement category
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0145] occurrences matches {0..1} matches {	-- Movement type
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0002] matches {	-- Patient class
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0073] occurrences matches {0..1} matches {	-- Assigned patient location
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0147] occurrences matches {0..1} matches {	-- Healthcare organisation
								include
									archetype_id/value matches {/.*/}
							}
							ELEMENT[at0074] occurrences matches {0..1} matches {	-- Point of care/Unit
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0079] occurrences matches {0..1} matches {	-- Bed
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0078] occurrences matches {0..1} matches {	-- Room
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0101] occurrences matches {0..1} matches {	-- Building
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0102] occurrences matches {0..1} matches {	-- Floor
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0103] occurrences matches {0..1} matches {	-- Location description
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0081] occurrences matches {0..1} matches {	-- Location type
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Admission type
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0023] occurrences matches {0..1} matches {	-- Pre-admit number
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0094] occurrences matches {0..1} matches {	-- Prior patient location
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0148] occurrences matches {0..1} matches {	-- Healthcare organisation
								include
									archetype_id/value matches {/.*/}
							}
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0074]	-- /data[Tree]/items[Assigned patient location]/items[Point of care/Unit]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0079]	-- /data[Tree]/items[Assigned patient location]/items[Bed]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0078]	-- /data[Tree]/items[Assigned patient location]/items[Room]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0101]	-- /data[Tree]/items[Assigned patient location]/items[Building]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0102]	-- /data[Tree]/items[Assigned patient location]/items[Floor]
							use_node ELEMENT /data[at0001]/items[at0073]/items[at0103]	-- /data[Tree]/items[Assigned patient location]/items[Location description]
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0081]	-- /data[Tree]/items[Assigned patient location]/items[Location type]
						}
					}
					allow_archetype CLUSTER[at0146] occurrences matches {0..1} matches {	-- Doctors
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {	-- Hospital service
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0049] occurrences matches {0..1} matches {	-- Admit source
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0061] occurrences matches {0..1} matches {	-- Financial class
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0066] occurrences matches {0..1} matches {	-- Charge price indicator
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[ac0001]}		-- Any term that 'is a' 'Charge price indicator'
							}
						}
					}
					ELEMENT[at0071] matches {	-- Admit date/time
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0133] occurrences matches {0..1} matches {	-- Accident type
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0134] occurrences matches {0..1} matches {	-- Movement number
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0137] occurrences matches {0..1} matches {	-- Treatment category
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0144] occurrences matches {0..1} matches {	-- Discharge date/time
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					CLUSTER[at0140] occurrences matches {0..1} matches {	-- Discharge to location
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0149] occurrences matches {0..1} matches {	-- Healthcare organisation
								include
									archetype_id/value matches {/.*/}
							}
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0074]	-- /data[Tree]/items[Assigned patient location]/items[Point of care/Unit]
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0079]	-- /data[Tree]/items[Assigned patient location]/items[Bed]
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0078]	-- /data[Tree]/items[Assigned patient location]/items[Room]
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0101]	-- /data[Tree]/items[Assigned patient location]/items[Building]
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0102]	-- /data[Tree]/items[Assigned patient location]/items[Floor]
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0103]	-- /data[Tree]/items[Assigned patient location]/items[Location description]
							use_node ELEMENT occurrences matches {0..1} /data[at0001]/items[at0073]/items[at0081]	-- /data[Tree]/items[Assigned patient location]/items[Location type]
						}
					}
					ELEMENT[at0136] occurrences matches {0..1} matches {	-- Discharge disposition
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0135] occurrences matches {0..1} matches {	-- Mode of arrival
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}


