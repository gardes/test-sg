definition
	EVALUATION[at0000] matches {	-- Skaderegistrering
		data matches {
			ITEM_TREE[at0001] matches {	-- Skaderegistrering minimum datasett
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Når
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- Kontakttype skade
						value matches {
							1|[local::at0004], 	-- 1 - Første episode starter mindre enn 7 dager etter en skade er inntruffet
							2|[local::at0005], 	-- 2 - Første episode starter 7 eller flere dager etter at en skade er inntruffet
							3|[local::at0006]  	-- 3 - Ny henvisningsperiode/episode for skade som har medført henvisnignsperidoe/episode tidligere
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Kontaktårsak skade
						value matches {
							1|[local::at0008], 	-- 1- Ulykkesskade
							2|[local::at0009], 	-- 2 - Villet egenskade
							3|[local::at0010], 	-- 3 - Vold, overfall
							8|[local::at0011], 	-- 8 - Skade etter lovhjemlet inngripen, krigshandling
							9|[local::at0012]  	-- 9 - Ukjent kontaktårsak
						}
					}
					ELEMENT[at0013] occurrences matches {0..1} matches {	-- Skademekanisme
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0014, 	-- 1 - Slag, støt pga. fall
									at0015, 	-- 2 - Slag, støt pga kontakt med menneske, dyr
									at0016, 	-- 3 - Klemt, fanget, knust
									at0017, 	-- 4 - Kutt, bitt, stikk, skutt
									at0018, 	-- 5 - Fremmedlegeme
									at0019, 	-- 6 - Kvelning, drukning
									at0020, 	-- 7 - Forgiftning, etsing
									at0021, 	-- 8 - Forbrenning, røykskader, forfrysning, nedkjøling
									at0022, 	-- 9 - Elektrisitet, stråling, støy, vibrasjon
									at0023, 	-- 10 - Akutt overbelasning
									at0024, 	-- 11 - Annen skademekanisme
									at0025]	-- 12 - Ukjent skademekanisme
								}
							}
						}
					}
					ELEMENT[at0026] occurrences matches {0..1} matches {	-- Aktivitet ved skade
						value matches {
							1|[local::at0027], 	-- 1 - Inntektsgivende arbeid
							2|[local::at0028], 	-- 2 - På vei til/fra arbeid
							3|[local::at0029], 	-- 3 - Utdanning
							4|[local::at0030], 	-- 4 - Sport, idrett, mosjon i utdanning
							5|[local::at0031], 	-- 5 - Idrett, sport og trening i fritid
							6|[local::at0032], 	-- 6 - Lek, hobby, fritidsaktivitet
							8|[local::at0033], 	-- 8 - Annen aktivitet
							9|[local::at0034]  	-- 9  -Ukjent aktivitet
						}
					}
					allow_archetype CLUSTER[at0105] occurrences matches {0..*} matches {	-- Lokale registreringer
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0036] occurrences matches {0..1} matches {	-- Arbeidsgivers bransje
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0037, 	-- 1 - Jordbruk, skogbruk og fiske
									at0038, 	-- 2 - Bergverksdrift og utvinning
									at0039, 	-- 3 - Industri
									at0040, 	-- 4 - Elektrisitets-, gass- damp- og varmtvannsforsyning
									at0041, 	-- 5 - Vannforsyning-, avløps- og renovasjonsvirksomhet
									at0042, 	-- 6 - Bygge- annleggsvirksomhet
									at0043, 	-- 7 - Varehandel, reparasjon av motorvogner
									at0044, 	-- 8 - Transport og lagring
									at0045, 	-- 9 - Overnattings- og serveringsvirksomhet
									at0046, 	-- 10 - Informasjon og kommunikasjon
									at0047, 	-- 11 - Finansierings- og forsikringsvirksomhet
									at0048, 	-- 12 - Omsetnig og drift av fast eiendom
									at0049, 	-- 13 - Faglig, vitenskapelig og teknisk tjenesteyting
									at0050, 	-- 14  -Forretningsmessig tjenesteyting
									at0051, 	-- 15 - Offentlig administrasjon og forsvar, og trygdeordringer underlagt offentlig forvaltning
									at0052, 	-- 16 - Undervisning
									at0053, 	-- 17 - Helse- og sosialtjenester
									at0054, 	-- 18 - Kulturell virksomhet, underholdning og fritidsaktiviteter
									at0055, 	-- 19 - Annen tjenesteyting
									at0056, 	-- 20 - Lønnet arbeid i private husholdninger
									at0057]	-- 21 - Internasjonale organisasjoner
								}
							}
						}
					}
					allow_archetype CLUSTER[at0106] occurrences matches {0..*} matches {	-- Alvorlighet ved skade 
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.ais_scale(-[a-zA-Z0-9_]+)*\.v1/}
					}
					ELEMENT[at0072] occurrences matches {0..1} matches {	-- Skadested
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0073, 	-- 1 - Vei, gate, fortau, gang- , sykkelvei
									at0074, 	-- 2 - Annet transportområde
									at0075, 	-- 3 - Bolig boligområde
									at0076, 	-- 4 - Jordbruksbedrift, skogbruk
									at0077, 	-- 5 - Institusjon, sykehus, syke-, adershjem, off.kontor
									at0078, 	-- 6 - Lekeplass
									at0079, 	-- 7 - Skole, høyskole, universitet, barnehage, familiebarnehage i privat hjem eller liknende skolefritidsordning
									at0080, 	-- 8 - Sport-, idrettsområde (også på skole/høyskole/universitet)
									at0081, 	-- 9 - Fri natur, hav, sjø og vann
									at0082, 	-- 10 - Annet skadested
									at0083]	-- 11 - Ukjent skadested
								}
							}
						}
					}
					ELEMENT[at0102] occurrences matches {0..1} matches {	-- Skadekommune
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0084] occurrences matches {0..1} matches {	-- Veitrafikkskade
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0085] occurrences matches {0..1} matches {	-- Har kjøretøy vært i bevegelse?
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							ELEMENT[at0086] occurrences matches {0..1} matches {	-- Fremkomstmiddel
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0087, 	-- 1 - Til fots
											at0088, 	-- 2 - Sykkel
											at0089, 	-- 3 - Moped
											at0090, 	-- 4 - Motorsykke
											at0091, 	-- 5 - Personbil/varebil
											at0092, 	-- 6 - Lastebil
											at0093, 	-- 7 - Buss
											at0094, 	-- 8 - Trikk/tog/bane
											at0095, 	-- 9 - AVT/firhjuling
											at0096, 	-- 10 - Annet spesifisert
											at0097]	-- 11 - Ukjent
										}
									}
								}
							}
							ELEMENT[at0098] occurrences matches {0..1} matches {	-- X-Koordinat
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::497]>
										list = <
											["1"] = <
												units = <"°">
											>
										>
									>
								}
							}
							ELEMENT[at0099] occurrences matches {0..1} matches {	-- Y-Koordinat
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::497]>
										list = <
											["1"] = <
												units = <"°">
											>
										>
									>
								}
							}
							allow_archetype CLUSTER[at0103] occurrences matches {0..*} matches {	-- Veitrafikkskade
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.skade_veitrafikk(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
					CLUSTER[at0100] occurrences matches {0..1} matches {	-- Produktulykke
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0101] occurrences matches {0..1} matches {	-- Er produktulykke?
								value matches {
									DV_BOOLEAN matches {
										value matches {True, False}
									}
								}
							}
							allow_archetype CLUSTER[at0104] occurrences matches {0..*} matches {	-- Produktskade
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.skade_produktskade(-[a-zA-Z0-9_]+)*\.v1/}
							}
						}
					}
				}
			}
		}
	}

ontology
	term_definitions = <
		["no"] = <
			items = <
				["at0000"] = <
					text = <"Skaderegistrering">
					description = <"Registrering av felles minimum datasett til NPR">
				>
				["at0001"] = <
					text = <"Skaderegistrering minimum datasett">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"Når">
					description = <"Dato og tid hvor skade oppstod">
				>
				["at0003"] = <
					text = <"Kontakttype skade">
					description = <"Angir kontakttype for skade  -hva mer skal stå her til informasjon?">
				>
				["at0004"] = <
					text = <"1 - Første episode starter mindre enn 7 dager etter en skade er inntruffet">
					description = <"*">
				>
				["at0005"] = <
					text = <"2 - Første episode starter 7 eller flere dager etter at en skade er inntruffet">
					description = <"*">
				>
				["at0006"] = <
					text = <"3 - Ny henvisningsperiode/episode for skade som har medført henvisnignsperidoe/episode tidligere">
					description = <"*">
				>
				["at0007"] = <
					text = <"Kontaktårsak skade">
					description = <"Angir om skade skyldes vold, ulykke, etc.">
				>
				["at0008"] = <
					text = <"1- Ulykkesskade">
					description = <"En ulykke er en ufrivillig hendelse forårsaket av en hurtigvirkende kraft eller påvirkning som kan ytre seg i en skade på kroppen. Her brukesulykkesbegrepet i videste forstand.">
				>
				["at0009"] = <
					text = <"2 - Villet egenskade">
					description = <"Selvpåført skade er villet egenskade som selvmordsforsøk, selvmord, villet forgiftning, villet overdose av alkohol, narkotika, tabelltter eller annen villet egenskade">
				>
				["at0010"] = <
					text = <"3 - Vold, overfall">
					description = <"*">
				>
				["at0011"] = <
					text = <"8 - Skade etter lovhjemlet inngripen, krigshandling">
					description = <"Skader som oppstår i sammenstøt med politi, ordensmakt">
				>
				["at0012"] = <
					text = <"9 - Ukjent kontaktårsak">
					description = <"*">
				>
				["at0013"] = <
					text = <"Skademekanisme">
					description = <"Angir akutt eller plutseling påvirkning mot kroppen av fysisk agens, som mekanisk energi, varme, elektrisitet, kjemikalier og stråling, og i en mengde eller størrelse som overstiger den menneskelige organismens toleransenivå. I noen tilfeller (drukning og forfrysning9 som et plutselig fravær av nødvendig agens som oksygen eller varme.

Kommentar: Selv om det ikke er noe skarpt skille mellom sykdom og skade, oppstår skaden nesten umiddelbart etter kontakt med den påvirkende faktoren.">
				>
				["at0014"] = <
					text = <"1 - Slag, støt pga. fall">
					description = <"Inkl. fall på samme eller til lavere plan, snubling, glidning, fall i trapp, også fall på rulleski, ski, skøyter, osv. Eksklusiv: - vrikke ankel uten fall (Q) ">
				>
				["at0015"] = <
					text = <"2 - Slag, støt pga kontakt med menneske, dyr">
					description = <"Omfatter slag og støt ved direkte eller indirekte kontakt med gjenstand, transportmiddel, person, dyr i hvile eller i bevegelse. Inklusiv: - kjørende, -transportmiddel,- fallende, kastet eller sparket gjenstand,- tramp, spark av dyr, slag av person. Eksklusiv: - bitt, hugg, stikk (N),- bitt, stikk av giftig. -dyr, plante (A) ">
				>
				["at0016"] = <
					text = <"3 - Klemt, fanget, knust">
					description = <"Klemt fast eller fanget i noe (f.eks. maskiner), knust, overkjørt.">
				>
				["at0017"] = <
					text = <"4 - Kutt, bitt, stikk, skutt">
					description = <"Klippet, skåret, saget,revet, hugget, stukket, annen inntrengning (prosjektil, flis, partikkel). Dyrebitt, klort, insektsstikk. Eksklusiv: - fremmedlegeme i øye, øre etc. (R)  - bitt, stikk av giftig dyr, plante (A) ">
				>
				["at0018"] = <
					text = <"5 - Fremmedlegeme">
					description = <"I naturlig kroppsåpning, inklusiv alle slags gjenstander som trenger inn i øye, nese, øre, munn. Eksklusiv: - sveiseblink (E) - kvelning pga. fremmedlegeme (K) ">
				>
				["at0019"] = <
					text = <"6 - Kvelning, drukning">
					description = <"Omfatter kvelning pga. luftmangel: blokkering av luftvei, begravd av snø, jord, korn etc., druknet, nærdruknet. ">
				>
				["at0020"] = <
					text = <"7 - Forgiftning, etsing">
					description = <"Omfatter forgiftning eller etsning med faste, flytende eller gassformige stoffer som følge av kortvarig eksponering (mindre enn 48 timer) Eksklusiv: Røykskade(T) ">
				>
				["at0021"] = <
					text = <"8 - Forbrenning, røykskader, forfrysning, nedkjøling">
					description = <"Berørt, eksponert for kulde eller varm væske, gjenstand, damp, ild og/eller røykgasser. Nedkjøling Eksklusiv: - Solforbrenning (E) ">
				>
				["at0022"] = <
					text = <"9 - Elektrisitet, stråling, støy, vibrasjon">
					description = <"Elektrisk strøm, lysbue, sveiseblink, elektrisk sjokk, solarium, solforbrenning, støy, vibrasjoner fra trykkluftsbor. Eksponeringen mindre enn 48 timer. ">
				>
				["at0023"] = <
					text = <"10 - Akutt overbelasning">
					description = <"Akutt overbelastning av kropp/kroppsdel, strekning, vridning pga. egen voldsom bevegelse eller anstrengende stilling. Eksklusiv: - Langvarig overbelastning mer enn 48 timer - Skade forårsaket av f.eks. slag, støt, klemming se F, S, L ">
				>
				["at0024"] = <
					text = <"11 - Annen skademekanisme">
					description = <"Naturkrefter, giftig dyr, plante ">
				>
				["at0025"] = <
					text = <"12 - Ukjent skademekanisme">
					description = <"*">
				>
				["at0026"] = <
					text = <"Aktivitet ved skade">
					description = <"*">
				>
				["at0027"] = <
					text = <"1 - Inntektsgivende arbeid">
					description = <"*">
				>
				["at0028"] = <
					text = <"2 - På vei til/fra arbeid">
					description = <"*">
				>
				["at0029"] = <
					text = <"3 - Utdanning">
					description = <"*">
				>
				["at0030"] = <
					text = <"4 - Sport, idrett, mosjon i utdanning">
					description = <"*">
				>
				["at0031"] = <
					text = <"5 - Idrett, sport og trening i fritid">
					description = <"*">
				>
				["at0032"] = <
					text = <"6 - Lek, hobby, fritidsaktivitet">
					description = <"*">
				>
				["at0033"] = <
					text = <"8 - Annen aktivitet">
					description = <"*">
				>
				["at0034"] = <
					text = <"9  -Ukjent aktivitet">
					description = <"*">
				>
				["at0036"] = <
					text = <"Arbeidsgivers bransje">
					description = <"Angis hvis aktivet ved skade er 1 eller 2">
				>
				["at0037"] = <
					text = <"1 - Jordbruk, skogbruk og fiske">
					description = <"Omfatter bruken av planteressursene og de animalske ressursene, inkludert aktiviteter i forbindelse meddyrking av vekster, husdyrhold og -avl, produksjon av tømmer og høsting/utnyttelse av andre planter, dyr og animalske produkter fra en gård eller fra naturen. ">
				>
				["at0038"] = <
					text = <"2 - Bergverksdrift og utvinning">
					description = <"Omfatter utvinning avmineraler som naturlig forekommer i fast form (kull og malm), i flytende form (råolje) og luftform (naturgass). Utvinningen kan skje ved forskjellige metoder som f.eks. under og over jorden, gjennom borehull eller ved havminedrift mv. ">
				>
				["at0039"] = <
					text = <"3 - Industri">
					description = <"Industriproduksjon omfatter fysisk ellerkjemisk omdanning av materialer, stoffer eller deler til nye produkter, selv om dette ikke kan brukes som det eneste kriteriet for industriproduksjon (jf. merknaden til avfallsbehandling  nedenfor), de omdannede materialene, stoffene ellerdeler av råmaterialer som stammer fra jordbruk, skogbruk, fiske, bergverksdrift eller fra annen industriproduksjon. Vesentlig omdanning eller renovering av produkter betraktes vanligvis som industriproduksjon. Enhetene innenfor industriproduksjon betegnes ofte som anlegg eller fabrikker og nytter typisk motordrevne maskiner og motordrevne håndverksmaskiner.">
				>
				["at0040"] = <
					text = <"4 - Elektrisitets-, gass- damp- og varmtvannsforsyning">
					description = <"Omfatter levering av elektrisitet, naturgass, damp,varmt vann o.l. via hovedledninger og rør. Omfanget av nettverket er ikke avgjørende. Distribusjon av elektrisitet, gass, damp varmt vann o.l. i industriparker eller beboelseshus er inkludert. Derfor omfatter dette næringshovedområde drift av elektriske installasjoner og gassinstallasjoner, som genererer, kontrollerer og distribuerer elektrisitet eller gass. Dessuten er levering av damp og aircondition inkludert. ">
				>
				["at0041"] = <
					text = <"5 - Vannforsyning-, avløps- og renovasjonsvirksomhet">
					description = <"Næringshovedområdet omfatter aktiviteter i forbindelse med håndtering (herunder innsamling, behandling og uskadeliggjøring inkl. fjerning) av forskjellige slags avfall f.eks. fast og ikke-fast industri- og husholdningsavfall, samt forurenset jord. Produktet fra avfallsbehandlingsprosessene og avløpsrensing kan enten bortføres eller brukes iandre produksjonsprosesser. ">
				>
				["at0042"] = <
					text = <"6 - Bygge- annleggsvirksomhet">
					description = <"Næringshovedområdet omfatter generell bygge- og anleggsvirksomhet og bygge- og anleggsvirksomhet som krever spesialisering. Den omfatter nybygging,reparasjoner, tilbygging og ombygging, oppføring av prefabrikerte bygninger eller konstruksjoner på stedet, samt bygninger og konstruksjoner av mer midlertidig karakter. Generell bygge- og anleggsvirksomhet omfatter oppføring av boligbygninger, kontor- og forretningsbygninger, lager- og industribygninger, offentlige bygninger, driftsbygninger til jordbruket mv. eller anlegg som veier, bruer, tunneler, jernbaner, lufthavner, havner og andre vannprosjekter, vanningssystemer, kloakksystemer, industrianlegg, rørledninger og kraftledninger, idrettsanlegg mv. Arbeidet kan utføres for egen regning eller på honorar- eller kontraktbasis.Deler av arbeidet og alt praktisk arbeid kan settes bort til underentreprenører. Enheter som har det overordnede ansvar for et byggeprosjekt er gruppert her. Næringshovedområdet omfatter også reparasjon og vedlikehold av bygninger og anlegg. Næringshovedområdet omfatter også utvikling av byggeprosjekter i forbindelse med bygge- og anleggsarbeider ved å samle de økonomiske, tekniske og fysiske midler som skal til for å gjennomføre byggeprosjektet for senere salg. Hvis disse aktivitetene ikke utføres for senere salg av byggeprosjektene, men derimot til eget bruk (f.eks. utleie av lokaler eller produksjonsvirksomhet) grupperes enhetene ikke her,men i hht. driften, dvs. eiendomsdrift, type produksjonsvirksomhet osv. ">
				>
				["at0043"] = <
					text = <"7 - Varehandel, reparasjon av motorvogner">
					description = <"Omfatter agentur-, engros- og detaljhandel med alletyper varer og tjenester i forbindelse med salg av varer.Salget må skje uten at selger har bearbeidet eller omdannet varen. Omfatter salg av både nye og brukte varer.">
				>
				["at0044"] = <
					text = <"8 - Transport og lagring">
					description = <"Næringshovedområdet omfatterpassasjer- og godstransport (med eller uten ruteplan) med jernbane, rørtransport, veitransport, skipsfart eller luftfart og tjenesteri tilknytning til dette som transportsentraler, godsterminaler, parkeringsanlegg og lagerdrift mv. Næringsområdet omfatter også utleie av transportutstyr med sjåfør eller operatør. Post- og kurervirksomhet er også inkludert. ">
				>
				["at0045"] = <
					text = <"9 - Overnattings- og serveringsvirksomhet">
					description = <"Omfatter tilbud om korttidsovernatting til gjester og turister samt servering av måltider og drikkevarer. Typen avservice og graden av service innenfor dette næringshovedområde kan være sterkt varierende. Næringshovedområdet omfatter ikke utleie av boliger på langtidsbasis som er gruppert under L Omsetning og drift av fast eiendom. Det omfatter heller ikke produksjon av mat- og drikkevarer som ikke er beregnet på umiddelbart forbruk eller som selges gjennom uavhengige distribusjonskanaler, dvs. via engroshandel eller butikkhandel. Produksjon av slike næringsmidler er gruppert under hovednæringsområde C Industri. ">
				>
				["at0046"] = <
					text = <"10 - Informasjon og kommunikasjon">
					description = <"Omfatter produksjon og formidling av informasjon og kulturelle produkter, levering av midler til overføring og formidling av disse produktene, samt data og  kommunikasjonstjenester og andre informasjonstjenester. Hovedelementene i dette næringshovedområdet er forlagsvirksomhet, film-, video- og fjernsynsprogramproduksjon, utgivelse av musikk- oglydopptak, radio- og fjernsynskringkasting, telekommunikasjon, tjenester tilknyttet informasjonsteknologi og informasjonstjenester. ">
				>
				["at0047"] = <
					text = <"11 - Finansierings- og forsikringsvirksomhet">
					description = <"Omfatter finansiell formidling, forsikring, gjenforsikring og pensjonskasser samt hjelpetjenester i forbindelse med slike enheter. ">
				>
				["at0048"] = <
					text = <"12 - Omsetnig og drift av fast eiendom">
					description = <"Omfatter bortforpaktere, agenter og/eller meglere på ett eller flere av følgende områder: kjøp eller salg av fast eiendom, utleie av fast eiendom, annen virksomhet i forbindelse med fast eiendom som f.eks.: taksering av fast eiendom og oppdrag som deponeringsagent i forbindelse med fast eiendom. Virksomhet i dette hovednærings- område kan utøves på egen eller leid eiendom og kan skje på honorar- eller kontraktbasis.">
				>
				["at0049"] = <
					text = <"13 - Faglig, vitenskapelig og teknisk tjenesteyting">
					description = <"Omfatter spesialisert faglig, vitenskapelig og teknisk tjenesteyting. Disse tjenestene krever en høy utdanning og stiller spesialisert kunnskap og spesielle ferdigheter til rådighet for brukerne. ">
				>
				["at0050"] = <
					text = <"14  -Forretningsmessig tjenesteyting">
					description = <"Omfatter en rekketjenester som støtter generelle forretningsområder. Disse tjenestene skiller seg fra tjenestene i næringshovedområde M ved at hovedformålet ikke er å overføre spesialisert kunnskap.">
				>
				["at0051"] = <
					text = <"15 - Offentlig administrasjon og forsvar, og trygdeordringer underlagt offentlig forvaltning">
					description = <"Omfatter offentlige aktiviteter som normalt utøves av offentlig forvaltning. Omfatter vedtak og rettslig tolkning av lover og forskrifter samt administrasjon av tiltak og planerbasert på disse rettslige aktiviteter, beskatning, forsvar, innvandringssaker, utenrikssaker og administrasjon av offentlige handlingsplaner og programmer. ">
				>
				["at0052"] = <
					text = <"16 - Undervisning">
					description = <"Omfatter undervisning på alle nivå og innenfor alle fag. Undervisningen kan være muntlig eller skriftlig og kan foregå via radio- eller tv-utsendinger via Internett ellerper korrespondanse. Omfatter både undervisning på de forskjellige institusjoner i det ordinære skolesystemet på de forskjellige nivå (grunnleggende undervisning) og voksen-  undervisning, lese- og skriveundervisning mv. ">
