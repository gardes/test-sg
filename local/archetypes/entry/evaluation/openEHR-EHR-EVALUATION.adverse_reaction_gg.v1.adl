							}
							ELEMENT[at0032] occurrences matches {0..1} matches {	-- Reaction Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0070] occurrences matches {0..*} matches {	-- sensitivityTest
						value matches {
							DV_URI matches {*}
						}
					}
				}
			}
		}
