						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0009] occurrences matches {0..*} matches {	-- Adverse Reaction Event
						items cardinality matches {3..*; unordered} matches {
							ELEMENT[at0010] occurrences matches {0..*} matches {	-- Specific Substance/Agent
								value matches {
									DV_TEXT matches {*}
								}
							}
