definition
	EVALUATION[at0000] matches {	-- GDL Rule result
		data matches {
			ITEM_LIST[at0001] matches {	-- List
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Result
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0017] occurrences matches {0..1} matches {	-- Guideline
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
		}
	}

