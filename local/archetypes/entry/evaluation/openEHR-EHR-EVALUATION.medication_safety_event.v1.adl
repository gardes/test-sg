									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0223, 	-- Low
											at0224, 	-- Medium
											at0225]	-- High
										}
									}
								}
							}
							CLUSTER[at0236] occurrences matches {0..*} matches {	-- Intervention details
								items cardinality matches {1..*; unordered} matches {
									CLUSTER[at0347] occurrences matches {0..*} matches {	-- Intervention additional details
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0012] occurrences matches {0..1} matches {	-- Intervention
