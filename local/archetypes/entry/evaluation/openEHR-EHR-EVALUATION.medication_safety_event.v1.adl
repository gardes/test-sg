													DV_CODED_TEXT matches {
														defining_code matches {
															[local::
															at0304, 	-- Prescriber informed
															at0305, 	-- Nurse informed/instructed
															at0306, 	-- Dispensing rejected
															at0307, 	-- Drug recalled from the ward
															at0308, 	-- Dietary regime changed
															at0309, 	-- Food supplement discontinued
															at0310, 	-- Food supplement initiated
															at0311, 	-- Reconstitution solvent changed
															at0312, 	-- Diluent changed
															at0313, 	-- Medical device suggested
															at0314, 	-- Drug changed
															at0315, 	-- Dose increased
															at0316, 	-- Dose decreased
															at0317, 	-- Dosage form changed
															at0318, 	-- Administration interval changed
															at0319, 	-- Drug discontinued
															at0320, 	-- Drug initiated
															at0321, 	-- Drug administration rate changed
															at0322, 	-- Route of administration changed
															at0323, 	-- Resuscitation required
															at0324, 	-- Heamodialysis required
															at0325, 	-- Transplantation required
															at0326, 	-- Transfusion required
															at0327, 	-- Operating procedure required
															at0328, 	-- Additional laboratory test(s) required
															at0329, 	-- Physiotherapy required
															at0330, 	-- Psychotherapy required
															at0331]	-- Other
														}
													}
												}
											}
											ELEMENT[at0348] occurrences matches {0..1} matches {	-- Comment
												value matches {
													DV_TEXT matches {*}
												}
											}
										}
									}
