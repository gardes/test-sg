definition
	EVALUATION[at0000] matches {	-- Medication safety event
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {2..*; unordered} matches {
					ELEMENT[at0235] occurrences matches {0..1} matches {	-- Main event for case?
						value matches {
							DV_BOOLEAN matches {
								value matches {True}
							}
						}
					}
					ELEMENT[at0002] occurrences matches {0..1} matches {	-- Safety event type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0250, 	-- Patient suffered from an adverse drug event
									at0269, 	-- Therapeutic inappropriateness of a patient's medication regimen
									at0252, 	-- Therapeutic duplication in the patient's medication regimen
									at0253, 	-- Variation from organizational criteria for use
									at0254, 	-- Wrong drug administered to a patient
									at0255, 	-- Inappropriate dosage selection
									at0256, 	-- Inappropriate timing of administration and/or dosing intervals
									at0257, 	-- Inappropriate route of administration
									at0258, 	-- Inappropriate application method of drug
									at0259, 	-- Drug-drug, Drug-food interactions
									at0260, 	-- Theraphy could be optimized
									at0261, 	-- Inappropriate duration of treatment
									at0262, 	-- Drug-disease or Drug-laboratory test interactions
									at0263, 	-- Allergies or cross sensitivities
									at0264, 	-- No drug prescribed for existing indication
									at0265, 	-- Contraindication
									at0333, 	-- Inappropriate drug form
									at0334]	-- Other
								}
							}
						}
					}
					ELEMENT[at0003] occurrences matches {0..1} matches {	-- MEDRA classification
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[ac0001]}		-- *New constraint(sl)
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- DateTime of event onset
						value matches {
							DV_DATE_TIME matches {*}
						}
					}
					ELEMENT[at0041] occurrences matches {0..1} matches {	-- Description of event
						value matches {
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0233] occurrences matches {0..*} matches {	-- Cause of event
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0062] occurrences matches {0..1} matches {	-- Estimated cause of event
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0067, 	-- Verbal miscommunication
											at0068, 	-- Written miscomunication-Illegible handwritting
											at0069, 	-- Written miscomunication-Abbreviations
											at0071, 	-- Written miscomunication-Non metric units of measurement
											at0072, 	-- Written miscomunication-Decimal point
											at0073, 	-- Misread or didnt read
											at0074, 	-- Misinterpretation of the order
											at0075, 	-- Sound-alike drug name
											at0076, 	-- Look-alike drug name
											at0077, 	-- Packaging-Inappropriate Packaging or Design
											at0078, 	-- Packing-Dosage form
											at0079, 	-- Packaging-Error due to similarity to another drug (colour, form, size)
											at0080, 	-- Packaging-Error due different strength
											at0081, 	-- Human factors-Lack of information/knowledge
											at0082, 	-- Work not accomplished
											at0083, 	-- Incomplete documentation
											at0084, 	-- Work overload, burn out syndrome
											at0085, 	-- Fatigue/Lack of sleep
											at0086, 	-- Confrontational or intimidating behaviour
											at0087, 	-- Inappropriate lighting
											at0088, 	-- Inappropriate temperature/humidity
											at0089, 	-- Frequent interuptions and distractions
											at0090, 	-- Lack of staff
											at0091]	-- Other
										}
									}
								}
							}
							ELEMENT[at0234] occurrences matches {0..1} matches {	-- Comment
								value matches {
									DV_TEXT matches {*}
								}
							}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- Related trigger
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0270, 	-- Transfusion of Blood or Use of Blood Products
									at0271, 	-- Acute Dialysis
									at0272, 	-- Positive Blood Culture
									at0273, 	-- X-Ray or Doppler Studies for Emboli or Deep Vein Thrombosis
									at0274, 	-- Decrease in Hemoglobin or Hematocrit of 25% or Greater
									at0275, 	-- Patient Fall
									at0276, 	-- Pressure Ulcers
									at0277, 	-- Readmission within 30 Days
									at0278, 	-- Restraint Use
									at0279, 	-- Healthcare-Associated Infections
									at0280, 	-- In-Hospital Stroke
									at0281, 	-- Clostridium difficile Positive Stool
									at0282, 	-- Partial Thromboplastin Time (PTT) Greater than 100 Seconds
									at0283, 	-- International Normalized Ratio (INR) Greater than 6
									at0284, 	-- Glucose Less than 50 mg/dl
									at0285, 	-- Rising BUN or Serum Creatinine Two Times (2x) over Baseline
									at0286, 	-- Vitamin K Administration
									at0287, 	-- Diphenhydramine (Benadryl) Administration
									at0288, 	-- Romazicon (Flumazenil) Administration
									at0289, 	-- Naloxone (Narcan) Administration
									at0290, 	-- Anti-Emetic Administration
									at0291, 	-- Over-Sedation/Hypotension
									at0292, 	-- Abrupt Medication Stop
									at0293, 	-- Intra- or Post-Operative Death
									at0294, 	-- Mechanical Ventilation Greater than 24 Hours Post-Operatively
									at0295, 	-- Intra-Operative Administration of Epinephrine, Norepinephrine, Naloxone, or Romazicon
									at0296, 	-- Pneumonia Onset
									at0297, 	-- Readmission to the Intensive Care Unit
									at0298, 	-- Terbutaline Use
									at0299, 	-- Platelet Count Less than 50,000
									at0300, 	-- Administration of Oxytocic Agents (such as oxytocin, methylergonovine, and 15-methyl-prostaglandin in the post-partum period)
									at0301, 	-- Administration of General Anesthesia
									at0302, 	-- Readmission to the ED within 48 Hours
									at0303, 	-- Time in ED Greater than 6 Hours
									at0332]	-- Unknown
								}
							}
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0018] occurrences matches {0..*} matches {	-- Adverse effect
						items cardinality matches {1..*; unordered} matches {
							CLUSTER[at0345] occurrences matches {0..*} matches {	-- Reaction details
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0021] occurrences matches {0..1} matches {	-- Reaction
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {[ac0003]}		-- *New constraint(sl)
											}
										}
									}
									ELEMENT[at0346] occurrences matches {0..1} matches {	-- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
							ELEMENT[at0108] occurrences matches {0..1} matches {	-- DateTime of reaction onset
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0222] occurrences matches {0..1} matches {	-- Severity
