			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0002.1] occurrences matches {0..*} matches {	-- Terminologija
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {[local::]}
							}
						}
					}
					allow_archetype ITEM[at0003.1] occurrences matches {0..1} matches {	-- Items
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0.8] occurrences matches {0..*} matches {	-- Aktivnosti so bile izvedene na predlog
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
