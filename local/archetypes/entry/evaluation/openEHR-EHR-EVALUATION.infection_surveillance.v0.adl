definition
	EVALUATION[at0000] matches {	-- Infection Surveillance
		data matches {
			ITEM_TREE[at0001] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					CLUSTER[at0062] occurrences matches {0..1} matches {	-- Investigation trigger
						items cardinality matches {1..*; unordered} matches {
							CLUSTER[at0008] occurrences matches {0..*} matches {	-- Organisms
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0002] matches {	-- Organism
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0003] occurrences matches {0..1} matches {	-- Pathogenic status
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0059, 	-- Confirmed Pathogen
													at0061]	-- Contaminant
												}
											}
										}
									}
								}
							}
							ELEMENT[at0020] occurrences matches {0..1} matches {	-- Original laboratory report
								value matches {
									DV_URI matches {*}
								}
							}
							ELEMENT[at0065] occurrences matches {0..1} matches {	-- Culture Date
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0054] occurrences matches {0..1} matches {	-- Radiology report
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0055] occurrences matches {0..1} matches {	-- Original radiology report
								value matches {
									DV_URI matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0009] occurrences matches {0..*} matches {	-- Acquisition
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.infection_acquisition(-[a-zA-Z0-9_]+)*\.v1/}
					}
					allow_archetype CLUSTER[at0047] occurrences matches {0..*} matches {	-- Specific infection details
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.blood_stream_infection(-[a-zA-Z0-9_]+)*\.v1/}
					}
					CLUSTER[at0052] occurrences matches {0..*} matches {	-- Associated Devices
						items cardinality matches {1..*; unordered} matches {
							allow_archetype CLUSTER[at0058] occurrences matches {0..*} matches {	-- Device
								include
									archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
							}
							ELEMENT[at0029] occurrences matches {0..*} matches {	-- Original Device information
								value matches {
									DV_URI matches {*}
								}
							}
							ELEMENT[at0043] occurrences matches {0..*} matches {	-- Time in situ
								value matches {
									DV_DURATION matches {*}
								}
							}
						}
					}
					CLUSTER[at0053] occurrences matches {0..*} matches {	-- Associated Procedures
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0026] occurrences matches {0..*} matches {	-- Contributing Procedure
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0056] occurrences matches {0..*} matches {	-- Date of Procedure
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0027] occurrences matches {0..*} matches {	-- Original Procedure information
								value matches {
									DV_URI matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0050] occurrences matches {0..*} matches {	-- Risk index
						include
							archetype_id/value matches {/.*/}
					}
					allow_archetype CLUSTER[at0057] occurrences matches {0..*} matches {	-- Diagnostic criteria
						include
							archetype_id/value matches {/.*/}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {	-- Same organism isolated from additional site?
						value matches {
							DV_BOOLEAN matches {
								value matches {True, False}
							}
						}
					}
					ELEMENT[at0039] occurrences matches {0..*} matches {	-- Additional site Laboratory report
						value matches {
							DV_URI matches {*}
						}
					}
					ELEMENT[at0051] occurrences matches {0..*} matches {	-- Associated investigation
						value matches {
							DV_URI matches {*}
						}
					}
					ELEMENT[at0022] occurrences matches {0..1} matches {	-- Outcome
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0046] occurrences matches {0..1} matches {	-- Comment
						value matches {
							DV_TEXT matches {*}
						}
					}
					allow_archetype ITEM[at0023] occurrences matches {0..*} matches {	-- Additional data
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0063] matches {	-- Tree
				items cardinality matches {0..*; unordered} matches {
					allow_archetype CLUSTER[at0064] occurrences matches {0..*} matches {	-- Validation criteria
						include
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}


