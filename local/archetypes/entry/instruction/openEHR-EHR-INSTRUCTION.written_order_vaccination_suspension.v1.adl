definition
	INSTRUCTION[at0000] matches {	-- Odločba za opustitev cepljenja
		activities cardinality matches {0..*; unordered} matches {
			ACTIVITY[at0002] occurrences matches {0..1} matches {	-- Trenutna aktivnost
				description matches {
					ITEM_TREE[at0003] matches {	-- *Tree(sl)
						items cardinality matches {1..*; unordered} matches {
							ELEMENT[at0004] occurrences matches {0..1} matches {	-- Datum odločbe o opustitvi cepljenja
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
							ELEMENT[at0005] occurrences matches {0..1} matches {	-- Številka odločbe o opustitvi cepljenja
								value matches {
									DV_TEXT matches {*}
								}
							}
							ELEMENT[at0022] occurrences matches {0..1} matches {	-- Status odločbe o opustitvi cepljenja
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0025, 	-- veljavno
											at0026]	-- v postopku tožbe
										}
									}
								}
							}
							ELEMENT[at0006] occurrences matches {0..1} matches {	-- Razlog za opustitev cepljenja
								value matches {
									DV_CODED_TEXT matches {
										defining_code matches {
											[local::
											at0007, 	-- alergija na sestavine cepiva
											at0008, 	-- resen neželen učinek cepiva po prehodnem odmerku cepiva
											at0009, 	-- bolezen ali zdravstveno stanje, ki je nezdružljivo s cepljenjem
											at0010, 	-- drugo
											at0011]	-- neznano
										}
									}
								}
							}
							CLUSTER[at0033] occurrences matches {1..*} matches {	-- Opustitev cepljenja
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- Tip opustitve cepljenja
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0017, 	-- začasno
													at0018, 	-- trajno
													at0019]	-- zavrnjeno
												}
											}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {	-- Datum opustitve cepljenja
										value matches {
											DV_DATE_TIME matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {	-- Trajanje opustitve cepljenja 
										value matches {
											DV_DURATION matches {*}
										}
									}
									ELEMENT[at0013] occurrences matches {0..1} matches {	-- Opustitev cepljenja z določenim cepivom
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {[ac0002]}		-- *New constraint(en)
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Opustitev cepljenja proti bolezni
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {[ac0001]}		-- *New constraint(en)
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

