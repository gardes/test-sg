	>

definition
	OBSERVATION[at0000] matches {	-- Parental growth
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									CLUSTER[at0004] occurrences matches {0..1} matches {	-- Mother
										items cardinality matches {1..*; unordered} matches {
											ELEMENT[at0006] occurrences matches {0..1} matches {	-- Age
												value matches {
													DV_DURATION matches {
														value matches {PYM/|P0Y..P150Y|}
													}
												}
											}
											ELEMENT[at0007] occurrences matches {0..1} matches {	-- Height
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::122]>
														list = <
															["1"] = <
																units = <"cm">
																precision = <|1|>
															>
															["2"] = <
																units = <"m">
																magnitude = <|>=0.0|>
																precision = <|3|>
															>
														>
													>
												}
											}
										}
									}
									CLUSTER[at0005] occurrences matches {0..1} matches {	-- Father
										items cardinality matches {1..*; unordered} matches {
											use_node ELEMENT occurrences matches {0..1} /data[at0001]/events[at0002]/data[at0003]/items[at0004]/items[at0006]	-- /data[Event Series]/events[Any event]/data[Tree]/items[Mother]/items[Age]
											use_node ELEMENT occurrences matches {0..1} /data[at0001]/events[at0002]/data[at0003]/items[at0004]/items[at0007]	-- /data[Event Series]/events[Any event]/data[Tree]/items[Mother]/items[Height]
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

