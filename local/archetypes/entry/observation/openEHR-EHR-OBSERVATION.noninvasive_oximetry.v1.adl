definition
	OBSERVATION[at0000] matches {	-- Pulse and non-invasive oximetry
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					POINT_EVENT[at0002] occurrences matches {0..1} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- SpO2 (pulse oximetry)
										value matches {
											DV_PROPORTION matches {
												numerator matches {|0.0..100.0|}
												denominator matches {|100.0|}
												type matches {2}
											}
										}
									}
									CLUSTER[at0042] occurrences matches {0..1} matches {	-- CO-oximetry
										items cardinality matches {0..*; unordered} matches {
											ELEMENT[at0044] occurrences matches {0..1} matches {	-- spOC
												value matches {
													C_DV_QUANTITY <
														property = <[openehr::119]>
														list = <
															["1"] = <
																units = <"ml/ml">
																magnitude = <|>=0.0|>
															>
														>
													>
												}
											}
											ELEMENT[at0045] occurrences matches {0..1} matches {	-- spCO
												value matches {
													DV_PROPORTION matches {
														numerator matches {|0.0..100.0|}
														denominator matches {|100.0|}
														is_integral matches {False}
														type matches {2}
													}
												}
											}
											ELEMENT[at0046] occurrences matches {0..1} matches {	-- spMet
												value matches {
													DV_PROPORTION matches {
														numerator matches {|0.0..100.0|}
														denominator matches {|100.0|}
														is_integral matches {False}
														type matches {2}
													}
												}
											}
										}
									}
									CLUSTER[at0041] occurrences matches {0..1} matches {	-- TC electrode oximetry
										items cardinality matches {0..*; unordered} matches {
											ELEMENT[at0039] occurrences matches {0..1} matches {	-- ptcO2
												value matches {
													DV_PROPORTION matches {
														numerator matches {|0.0..100.0|}
														denominator matches {|100.0|}
														is_integral matches {False}
														type matches {2}
													}
												}
											}
											ELEMENT[at0040] occurrences matches {0..1} matches {	-- ptcCO2
												value matches {
													DV_PROPORTION matches {
														numerator matches {|0.0..100.0|}
														denominator matches {|100.0|}
														is_integral matches {False}
														type matches {2}
													}
												}
											}
										}
									}
									ELEMENT[at0019] occurrences matches {0..*} matches {	-- Multimedia image
										value matches {
											DV_MULTIMEDIA matches {
												media_type matches {[openEHR::]}
											}
										}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {	-- Comment
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
						state matches {
							ITEM_TREE[at0014] matches {	-- Tree
								items cardinality matches {0..*; unordered} matches {
									ELEMENT[at0033] occurrences matches {0..1} matches {	-- Body position
										value matches {
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0034] occurrences matches {0..1} matches {	-- Exertion
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.level_of_exertion(-[a-zA-Z0-9_]+)*\.v1/}
									}
									allow_archetype CLUSTER[at0015] occurrences matches {0..1} matches {	-- Ambient oxygen
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.ambient_oxygen(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0016] occurrences matches {0..*} matches {	-- Confounding factors
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0007] matches {	-- List
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Sensor site
						value matches {
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- Sensor type
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0048, 	-- Transmittal
									at0049, 	-- Reflective
									at0050, 	-- Clamp
									at0051, 	-- Strip
									at0052]	-- Electrode
								}
							}
							DV_TEXT matches {*}
						}
					}
					CLUSTER[at0047] occurrences matches {0..1} matches {	-- Transcutaneous electrode
						items cardinality matches {0..*; unordered} matches {
							ELEMENT[at0037] occurrences matches {0..1} matches {	-- Operating temperature
								value matches {
									C_DV_QUANTITY <
										property = <[openehr::127]>
										list = <
											["1"] = <
												units = <"°C">
												magnitude = <|0.0..100.0|>
												precision = <|1|>
											>
										>
									>
								}
							}
							ELEMENT[at0038] occurrences matches {0..1} matches {	-- Date time applied
								value matches {
									DV_DATE_TIME matches {*}
								}
							}
						}
					}
					allow_archetype CLUSTER[at0018] occurrences matches {0..1} matches {	-- Device
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
					}
				}
			}
		}
	}


ontology
	terminologies_available = <"SNOMED-CT", ...>
	term_definitions = <
		["de"] = <
			items = <
				["at0000"] = <
					text = <"*Pulse and non-invasive oximetry(en)">
					description = <"*Measure of gas saturation in the blood such as SpO2, pTCO2 as recorded using pulse oximetery, transcutaneous oximetry or other non-invasive device/methodology. Primarily for oxygen saturations but other gases may be included.(en)">
					comment = <"*Pulse oximetry provides estimates of arterial oxyhemoglobin saturation (SaO2) by utilizing selected wavelengths of light to noninvasively determine the saturation of oxyhemoglobin (SpO2) using a pulse oximeter or equivalent non-invasive device.(en)">
				>
				["at0001"] = <
					text = <"Event Series">
					description = <"@ internal @">
				>
				["at0002"] = <
					text = <"*Any event(en)">
					description = <"*Timing event.(en)">
				>
				["at0003"] = <
					text = <"Baum">
					description = <"@ internal @">
				>
				["at0006"] = <
					text = <"*SpO2 (pulse oximetry)(en)">
					description = <"*The saturation of oxygen in the peripheral blood, measured by pulse oximetry.(en)">
					comment = <"*SpO2 is defined as the ratio of oxyhaemoglobin (HbO2) to the total concentration of haemoglobin (HbO2 + deoxyhaemoglobin).(en)">
				>
				["at0007"] = <
					text = <"Liste">
					description = <"@ internal @">
				>
				["at0009"] = <
					text = <"*Sensor site(en)">
					description = <"*The site of the measurement sensor.(en)">
				>
				["at0014"] = <
					text = <"*Tree(en)">
					description = <"*@ internal @(en)">
				>
				["at0015"] = <
					text = <"*Ambient oxygen(en)">
					description = <"*Details of the amount of oxygen being delivered to the subject at the time of observation.  Assumed values of 21% oxygen concentration, Fi02 of 0.21 and oxygen flow rate of 0 l/min or 0 ml/min.(en)">
				>
				["at0016"] = <
					text = <"*Confounding factors(en)">
					description = <"*Comment on and record other incidental factors that may be affect interpretation of the observation. For example, motion, pain, feeding in infants, tracheostomy.(en)">
				>
				["at0018"] = <
					text = <"*Device(en)">
					description = <"*Details of the non-invasive oximetry device used.(en)">
				>
				["at0019"] = <
					text = <"*Multimedia image(en)">
					description = <"*Details of a series of oximetry readings expressed as a multimedia image or series of images.  Pulse oximetry waveforms are often used to assess the quality of the SpO2 reading. In other circumstances the pulse waveform is used in direct clinical asessment e.g. as part of an assessment of cardiac capacity - in this case the waveform might reasonably be recorded within the OBSERVATION.pulse archetype or future equivalent.(en)">
					comment = <"*SpO2 waveform is prinicpally used as a visual guide to signal quality - i.e. indicates presence of confounders.(en)">
				>
				["at0020"] = <
					text = <"*Sensor type(en)">
					description = <"*The type of measurement sensor used e.g. transmittal, reflective, clamp or strip, electrode.(en)">
				>
				["at0033"] = <
					text = <"*Position(en)">
					description = <"*The position of the subject at the time of measurement.(en)">
				>
				["at0034"] = <
					text = <"*Exertion(en)">
					description = <"*Details about physical activity undertaken at the time of measurement.(en)">
				>
				["at0036"] = <
					text = <"*Comment(en)">
					description = <"*A text comment about the non-invasive oximetry result.(en)">
				>
				["at0037"] = <
					text = <"*Operating temperature(en)">
					description = <"*The operating temperature of a transcutaneous oximeter electrode.(en)">
				>
				["at0038"] = <
					text = <"*Date time applied(en)">
					description = <"*The date and time at which a transcutaneous oximetry electrode was first applied.(en)">
				>
				["at0039"] = <
					text = <"*ptcO2(en)">
					description = <"*The saturation of oxygen in the peripheral blood, measured by transcutaneous electrode oximetry. as ptcO2.(en)">
				>
				["at0040"] = <
					text = <"*ptcCO2(en)">
					description = <"*The saturation of carbon dioxide in the peripheral blood, measured by transcutaneous electrode oximetry.(en)">
				>
				["at0041"] = <
					text = <"*TC electrode oximetry(en)">
					description = <"*Blood gas measurements confined to transcutaneous electrode oximetry.(en)">
					comment = <"*Used by neonatal intensive care units to track precise TcPO2, and TcPCO2, levels noninvasively.  Not recommended as a substitute for arterial blood gas analysis in adults, but as an adjunct that will show trends in TcPO2 and TcPCO2 levels.(en)">
				>
				["at0042"] = <
					text = <"*CO-oximetry(en)">
					description = <"*Blood gas measurements confined to pulse CO-oximetry.(en)">
				>
				["at0044"] = <
					text = <"*spOC(en)">
					description = <"*Oxygen Content recorded with pulse CO-oximetry methodology.(en)">
				>
				["at0045"] = <
					text = <"*spCO(en)">
					description = <"*The saturation of carboxyhaemoglobin in the peripheral blood.(en)">
				>
				["at0046"] = <
					text = <"*spMet(en)">
					description = <"*The saturation of methoxyhaemoglobin in the peripheral blood.(en)">
				>
				["at0047"] = <
					text = <"*Transcutaneous electrode(en)">
					description = <"*Details of transcutaneous oximetry methodology.(en)">
				>
				["at0048"] = <
					text = <"*Transmittal(en)">
					description = <"*A pulse oximetry transmittal sensor was applied.(en)">
				>
				["at0049"] = <
					text = <"*Reflective(en)">
					description = <"*A pulse oximetry reflective sensor was applied.(en)">
				>
				["at0050"] = <
					text = <"*Clamp(en)">
					description = <"*A pulse oximetry clamp sensor was applied.(en)">
				>
				["at0051"] = <
					text = <"*Strip(en)">
					description = <"*A pulse oximetry strip sensor was applied.(en)">
				>
				["at0052"] = <
					text = <"*Electrode(en)">
					description = <"*A transcutaneous electrode sensor was applied.(en)">
				>
			>
		>
