	other_contributors = <"Sam Heard, Ocean Informatics, Australia", "Heather Leslie, Ocean Informatics, Australia">
	other_details = <
		["MD5-CAM-1.0.1"] = <"CB3A54C94B1BC1609D6787051163E962">
	>

definition
	OBSERVATION[at0000.1] matches {	-- Vnesene tekocine
		data matches {
			HISTORY[at0001] matches {	-- Event Series
				events cardinality matches {1..*; unordered} matches {
					EVENT[at0002] occurrences matches {0..*} matches {	-- Any event
						data matches {
							ITEM_TREE[at0003] matches {	-- *List(en)
								items cardinality matches {1..*; unordered} matches {
									ELEMENT[at0034.1] matches {	-- Način vnosa
										value matches {
											DV_CODED_TEXT matches {
												defining_code matches {
													[local::
													at0.38, 	-- Trajna infuzija-IV tekočina
													at0.37, 	-- Hrana - oralno
													at0.40, 	-- Hrana - enteralno
													at0.59, 	-- Pijača - oralno
													at0.41, 	-- Hrana - parenteralno
													at0.39, 	-- Zdravila - per os
													at0.51, 	-- Zdravila - IV
													at0.50, 	-- Transfuzija
													at0.47, 	-- Transfuzija - koncentrirani eritrociti
													at0.48, 	-- Transfuzija - krvotvorne matične celice
													at0.49, 	-- Transfuzija - plazma
													at0.52, 	-- Transfuzija-trombocitni koncentrat
													at0.53, 	-- *Dialysis(en)
													at0.55, 	-- *Continuous Veno-Venous Hemofiltration (CVVH) (en)
													at0.56, 	-- *CVAH(en)
													at0.54, 	-- *Plasmapheresis(en)
													at0.57, 	-- *Continuous veno-venous haemodiafiltration (CVVHDF)(en)
													at0.58]	-- *Peritoneal dialysis (PD)(en)
												}
											}
											DV_TEXT matches {*}
										}
									}
									allow_archetype CLUSTER[at0033] occurrences matches {0..1} matches {	-- Tujek vnosa
										include
											archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
									}
									ELEMENT[at0036] occurrences matches {0..1} matches {	-- Opis
										value matches {
											DV_TEXT matches {*}
										}
									}
									ELEMENT[at0035] occurrences matches {0..1} matches {	-- Količina
										value matches {
											C_DV_QUANTITY <
												property = <[openehr::129]>
												list = <
													["1"] = <
														units = <"ml">
													>
													["2"] = <
														units = <"l">
														magnitude = <|>=0.0|>
													>
													["3"] = <
														units = <"fl oz">
													>
												>
											>
										}
									}
									ELEMENT[at0032] occurrences matches {0..1} matches {	-- Opombe
										value matches {
											DV_TEXT matches {*}
										}
									}
								}
							}
						}
					}
					INTERVAL_EVENT[at0006] occurrences matches {0..*} matches {	-- Skupno
						math_function matches {
							DV_CODED_TEXT matches {
								defining_code matches {[openehr::148]}
							}
						}
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]	-- /data[Event Series]/events[Any event]/data[*List(en)]
						}
					}
					INTERVAL_EVENT[at0007] occurrences matches {0..*} matches {	-- Dejanski
						math_function matches {
							DV_CODED_TEXT matches {
								defining_code matches {[openehr::640]}
							}
						}
						data matches {
							use_node ITEM_TREE /data[at0001]/events[at0002]/data[at0003]	-- /data[Event Series]/events[Any event]/data[*List(en)]
						}
					}
				}
			}
		}
		protocol matches {
			ITEM_TREE[at0008] matches {	-- *List(en)
				items cardinality matches {0..*; unordered} matches {
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Natančnost meritve
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0018, 	-- Ocenjen
									at0019, 	-- Izmerjen neposredno
									at0020]	-- Potrjen
								}
							}
						}
					}
					allow_archetype CLUSTER[at0028] occurrences matches {0..1} matches {	-- Merilna naprava
						include
							archetype_id/value matches {/openEHR-EHR-CLUSTER\.device(-[a-zA-Z0-9_]+)*\.v1/}
						exclude
							archetype_id/value matches {/.*/}
					}
				}
			}
		}
	}

