	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To record findings of, and measurements of actual or estimated fluid input.">
			use = <"Use to record fluid input as part of routine nursing care.
May be used for descriptive assessments of input or to record detailed amounts and totals over a period.

">
			keywords = <"fluid", "fluid balance", "input output", "io", "input">
			misuse = <"Do not use as a primary record of the administration of fluids e.g intravascular, dialyisis. May be used alongside administration recrods ,normally an ACTION.">
			copyright = <"© openEHR Foundation">
		>
		["sl"] = <
			language = <[ISO_639-1::sl]>
			purpose = <"">
			use = <"">
			misuse = <"">
			copyright = <"© openEHR Foundation">
		>
	>
