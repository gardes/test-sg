	>
	details = <
		["en"] = <
			language = <[ISO_639-1::en]>
			purpose = <"To records details of the administration of flexible (or Functional) insulin therapy , including therapy administered by insulin pump.">
			use = <"">
			keywords = <"insulin", "diabetes", "pump">
			misuse = <"">
