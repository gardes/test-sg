definition
	CLUSTER[at0000] matches {	-- Insulin infusion setup procedure
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0017] occurrences matches {0..1} matches {	-- Date last changed
				value matches {
					DV_DATE_TIME matches {*}
				}
			}
			ELEMENT[at0018] occurrences matches {0..1} matches {	-- Frequency
				value matches {
					DV_DURATION matches {
						value matches {PD/|>=P0D|}
					}
				}
			}
			ELEMENT[at0019] occurrences matches {0..1} matches {	-- Priming volume
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::385]>
						list = <
							["1"] = <
								units = <"IU">
								magnitude = <|>=0.0|>
								precision = <|0|>
							>
						>
					>
				}
			}
			ELEMENT[at0020] occurrences matches {0..1} matches {	-- Skin cleaner
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0021] occurrences matches {0..1} matches {	-- Skin care ointment
				value matches {
					DV_TEXT matches {*}
				}
			}
		}
	}

