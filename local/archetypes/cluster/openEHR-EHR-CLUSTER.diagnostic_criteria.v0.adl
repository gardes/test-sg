definition
	CLUSTER[at0000] matches {	-- Diagnostic criteria
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001] occurrences matches {0..*} matches {	-- Diagnostic criteria
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype ITEM[at0002] occurrences matches {0..*} matches {	-- Additional criteria
				include
					archetype_id/value matches {/.*/}
