				}
			}
		}
	}


ontology
	term_definitions = <
		["en"] = <
			items = <
				["at0000"] = <
					text = <"Diagnostic criteria">
					description = <"A set of criteria that indicate what symptoms, signs or results must be present (and for how long) in order to qualify for a diagnosis (called inclusion criteria) as well as those symptoms that must not be present (called exclusion criteria) in order for an individual to qualify for a particular diagnosis.">
				>
				["at0001"] = <
					text = <"Diagnostic criteria">
					description = <"Set of criteria to support the making of a diagnosis.">
				>
				["at0002"] = <
					text = <"Additional criteria">
					description = <"Slot to contain related Diagnostic Criteria archetypes.">
				>
				["at0003"] = <
					text = <"Comment">
					description = <"*">
				>
			>
		>
	>
