definition
	CLUSTER[at0000.1] matches {    -- Examination of a tooth
		items cardinality matches {1..*; unordered} matches {
			ELEMENT[at0001.1] matches {    -- System or structure examined
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::at0.1]    -- Tooth
						}
					}
				}
			}
			ELEMENT[at0.40] occurrences matches {0..1} matches {    -- Tooth label
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0012.1] occurrences matches {0..1} matches {    -- Position
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0011.1] occurrences matches {0..*} matches {    -- Structured position
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.anatomical_location(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_circle(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.anatomical_location_relative(-[a-zA-Z0-9_]+)*\.v1/}
			}
			ELEMENT[at0002] occurrences matches {0..1} matches {    -- No abnormality detected
				value matches {
					DV_BOOLEAN matches {
						value matches {true}
					}
				}
			}
			ELEMENT[at0003] occurrences matches {0..1} matches {    -- Clinical description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.2] occurrences matches {0..1} matches {    -- Present?
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.3,    -- Present
							at0.4,    -- Extracted
							at0.5]    -- Unerupted
						}
					}
				}
			}
			ELEMENT[at0.6] occurrences matches {0..1} matches {    -- Restoration?
				value matches {
					DV_CODED_TEXT matches {
						defining_code matches {
							[local::
							at0.7,    -- Restored
							at0.8]    -- Unrestored
						}
					}
				}
			}
			allow_archetype CLUSTER[at0.39] occurrences matches {0..*} matches {    -- Mobility
				include
					archetype_id/value matches {/.*/}
			}
			CLUSTER[at0.9] occurrences matches {0..*} matches {    -- Tooth details
				items cardinality matches {1..*; unordered} matches {
					ELEMENT[at0.10] occurrences matches {0..1} matches {    -- Finding
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.11,    -- Cavity
									at0.12,    -- Amalgam
									at0.13,    -- Gold inlay
									at0.14,    -- Gold foil
									at0.15,    -- Other metal restoration
									at0.16,    -- Composite resin
									at0.17,    -- Traumatic avulsion
									at0.18,    -- Fractured crown
									at0.19,    -- Root tip
									at0.20,    -- Rotated
									at0.21,    -- Root canal filling
									at0.22,    -- Apicoectomy
									at0.23,    -- Intermediate restoration
									at0.24,    -- Crown - temporary
									at0.25,    -- Crown - full
									at0.26,    -- Crown - partial
									at0.27]    -- Crown veneer
								}
							}
							DV_TEXT matches {*}
						}
					}
					ELEMENT[at0.28] occurrences matches {0..2} matches {    -- Surface
						value matches {
							DV_CODED_TEXT matches {
								defining_code matches {
									[local::
									at0.29,    -- Mesial
									at0.30,    -- Distal
									at0.31,    -- Buccal
									at0.32,    -- Lingual
									at0.33,    -- Incisal
									at0.34]    -- Occlusal
								}
							}
						}
					}
					ELEMENT[at0.35] occurrences matches {0..1} matches {    -- Description
						value matches {
							DV_TEXT matches {*}
						}
					}
				}
			}
			ELEMENT[at0.37] occurrences matches {0..1} matches {    -- Gum description
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0.38] occurrences matches {0..1} matches {    -- Pocket depth
				value matches {
					C_DV_QUANTITY <
						property = <[openehr::122]>
						list = <
							["1"] = <
								units = <"mm">
								magnitude = <|0.0..<10.0|>
							>
						>
					>
				}
			}
			allow_archetype CLUSTER[at0004] occurrences matches {0..*} matches {    -- Examination findings
				include
					archetype_id/value matches {/.*/}
			}
			allow_archetype CLUSTER[at0005] occurrences matches {0..*} matches {    -- Multimedia representation
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v1|openEHR-EHR-CLUSTER\.multimedia(-[a-zA-Z0-9_]+)*\.v0/}
			}
			ELEMENT[at0006] occurrences matches {0..*} matches {    -- Clinical interpretation
				value matches {
					DV_TEXT matches {*}
				}
			}
			ELEMENT[at0007] occurrences matches {0..1} matches {    -- Comment
				value matches {
					DV_TEXT matches {*}
				}
			}
			allow_archetype CLUSTER[at0008] occurrences matches {0..*} matches {    -- Examination not done
				include
					archetype_id/value matches {/openEHR-EHR-CLUSTER\.exclusion_exam(-[a-zA-Z0-9_]+)*\.v1/}
			}
		}
	}


