										units = <"%">
									>
								>
							>
						}
					}
					ELEMENT[at0030] occurrences matches {0..1} matches {	-- BMI at presentation
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::349]>
								list = <
									["1"] = <
										units = <"kg/m2">
									>
								>
							>
						}
					}
					ELEMENT[at0031] occurrences matches {0..1} matches {	-- Smoking status
						value matches {
