						}
					}
					ELEMENT[at0028] occurrences matches {0..1} matches {	-- Weight at presentation
						value matches {
							C_DV_QUANTITY <
								property = <[openehr::124]>
								list = <
									["1"] = <
										units = <"kg">
									>
									["2"] = <
										units = <"g">
									>
								>
							>
						}
					}
					ELEMENT[at0029] occurrences matches {0..1} matches {	-- Ht at presentation
